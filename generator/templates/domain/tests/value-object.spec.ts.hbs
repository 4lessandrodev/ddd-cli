import MyValueObject, { Props } from '@modules/my-module/domain/value-object';

describe('value-object', () => {
	describe('success cases', () => {
		const result = MyValueObject.create('valid_value');
		expect(result.isOk()).toBeTruly();
	});

	describe('failure cases', () => {
		const result = MyValueObject.create(undefined as unknown as string);
		expect(result.isFail()).toBeTruly();
	});
})
